namespace Backend.Models
{
    public class User
    {
        public string id { get; set; }
        public string username { get; set; }
        public string password { get; set; }
        public UserRole role { get; set; }
    }

    public enum UserRole
    {
        Student = 'student',
        Lecturer = 'lecturer',
        Admin = 'admin'
    }
}